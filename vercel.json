{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
} 